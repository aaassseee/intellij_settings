<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
  </component>
  <component name="TodoConfiguration">
    <pattern useCustomColors="true" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="ffff00" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="ffff00" />
    </pattern>
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\btodo\b.*\(low\).*">
      <option name="FOREGROUND" value="ff00" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="ff00" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\btodo\b.*\(high\).*">
      <option name="FOREGROUND" value="ff0000" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="ff0000" />
    </pattern>
    <pattern icon="question" useCustomColors="true" pattern="\btodo\b.*\(check\).*">
      <option name="FOREGROUND" value="7fff" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="7fff" />
    </pattern>
  </component>
</application>